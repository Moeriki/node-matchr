{
  "name": "matchr",
  "version": "1.1.1",
  "description": "Powerful JavaScript value matching.",
  "keywords": [
    "matcher",
    "array",
    "date",
    "object",
    "regex",
    "type"
  ],
  "author": "Dieter Luypaert <dieterluypaert@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:Moeriki/node-matchr.git",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint *.js test/",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "dependencies": {
    "pretty-format": "~18.1.0",
    "stringify-object": "~3.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "eslint": "~3.14.1",
    "eslint-config-muriki": "~8.2.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jasmine": "~2.2.0",
    "eslint-plugin-node": "~3.0.5",
    "husky": "~0.13.1",
    "jest-cli": "^18.1.0",
    "lint-staged": "^3.2.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.test\\.js$"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
