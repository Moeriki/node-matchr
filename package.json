{
  "name": "matchr",
  "version": "1.1.0",
  "description": "Powerful JavaScript value matching.",
  "keywords": [
    "matcher",
    "array",
    "date",
    "object",
    "regex",
    "type"
  ],
  "author": "Dieter Luypaert <dieterluypaert@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:Moeriki/node-matchr.git",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint *.js __tests__/",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "dependencies": {
    "stringify-object": "~3.1.0"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "^2.11.15",
    "eslint": "~3.12.2",
    "eslint-config-muriki": "~8.1.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jasmine": "~2.2.0",
    "eslint-plugin-node": "~3.0.5",
    "husky": "^0.12.0",
    "jest-cli": "~18.1.0",
    "lint-staged": "~3.2.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.test\\.js$"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
